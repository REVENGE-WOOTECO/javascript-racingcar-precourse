{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["prettier"],
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "import/prefer-default-export": "off", 
        "import/extensions": [ "off" ]
    }
}
